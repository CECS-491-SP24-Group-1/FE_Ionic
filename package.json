{
	"name": "wraithapp",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"fmt": "npx prettier --write . && cd vaultlib_go && make fmt",
		"preview": "vite preview",
		"check-types": "tsc --noEmit",
		"ionic:serve": "vite dev --host",
		"dev:android": "ionic capacitor run android --external --livereload -p 5173 --consolelogs --serverlogs",
		"dev:ios": "ionic capacitor run ios --external --livereload -p 5173",
		"android": "npm run build && cap run android --verbose",
		"vaultlib": "cd vaultlib_go && make fmt && make build && make install",
		"sync": "cap sync",
		"android_logs": "adb logcat -T '1' | grep --line-buffered 'Capacitor/Console' | sed -n 's/.*File: //p'"
	},
	"dependencies": {
		"@capacitor/android": "^6.1.2",
		"@capacitor/app": "^6.0.1",
		"@capacitor/core": "^6.1.2",
		"@capacitor/haptics": "^6.0.1",
		"@capacitor/ios": "^6.1.2",
		"@capacitor/keyboard": "^6.0.2",
		"@capacitor/status-bar": "^6.0.1",
		"@ionic/react": "^8.2.7",
		"@ionic/react-router": "^8.2.7",
		"@types/express": "^5.0.0",
		"@types/ws": "^8.5.12",
		"axios": "^1.7.7",
		"express": "^4.21.0",
		"framer-motion": "^11.5.4",
		"immer": "^10.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"react-toastify": "^10.0.5",
		"socket.io-client": "^4.8.0",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"@capacitor/cli": "^6.1.2",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.45",
		"prettier": "^3.3.3",
		"sass-embedded": "^1.78.0",
		"scss": "^0.2.4",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.5.4",
		"vite": "^5.4.3",
		"vite-plugin-pwa": "^0.20.5",
		"vite-tsconfig-paths": "^5.0.1"
	}
}
