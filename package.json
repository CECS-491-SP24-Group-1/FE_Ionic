{
	"name": "wraithapp",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check-types": "tsc --noEmit",
		"ionic:serve": "vite dev --host",
		"dev:android": "ionic capacitor run android --external --livereload -p 5173",
		"dev:ios": "ionic capacitor run ios --external --livereload -p 5173",
		"android": "cap run android --verbose",
		"vaultlib": "cd vaultlib_go && make build && make install",
		"sync": "cap sync"
	},
	"dependencies": {
		"@capacitor/android": "^6.1.2",
		"@capacitor/app": "^6.0.1",
		"@capacitor/cli": "^6.1.2",
		"@capacitor/core": "^6.1.2",
		"@capacitor/haptics": "^6.0.1",
		"@capacitor/ios": "^6.1.2",
		"@capacitor/keyboard": "^6.0.2",
		"@capacitor/status-bar": "^6.0.1",
		"@ionic/react": "^8.2.7",
		"@ionic/react-router": "^8.2.7",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react": "^4.3.1",
		"typescript": "^5.5.4",
		"vite": "^5.4.3"
	}
}
